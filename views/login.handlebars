<div class="container box" id="sign-in-box">
    <div id="sign-in-title">
        <h3 id="sign-in-header">Sign In</h3>
    </div>
    <form id="signup" name="signup" method="post" action="/login">
    <div class="form-group row">
    <label for="inputUsername" class="col-sm-2 col-form-label logins">Username</label>
    <div class="col-sm-10">
      <input type="username" class="form-control form-control-lg" id="login-name" placeholder="Username">
    </div>
  </div>
  <div class="form-group row">
    <label for="inputPassword" class="col-sm-2 col-form-label logins">Password</label>
    <div class="col-sm-10">
      <input type="password" class="form-control form-control-lg" id="login-pw" placeholder="Password">
    </div>
    
  </div>
  <div id="login-btn-parent">
  <button id="login-btn" type="button" class="btn btn-outline-primary">Sign In</button> 
  </div>
  </form>



